"metadata": {"non-ghost_vars": {"longitude": {"name_one": "Lon.", "dp": 4},
                                "latitude":  {"name_one": "Lat.", "dp": 4}},
             "ghost_vars": {"longitude":                           {"name_one": "Lon.", "dp": 4},
                            "latitude":                            {"name_one": "Lat.", "dp": 4},
                            "measurement_altitude":                {"name_one": "Meas. Alt.", "name_multiple": "Median Meas. Alt.", "dp": 2, "units": "m"},
                            "GSFC_coastline_proximity":            {"name_one": "To Coast", "name_multiple": "Median To Coast", "dp": 2, "units": "km"},
                            "GHSL_population_density":             {"name_one": "Pop. Dens.", "name_multiple": "Median Pop. Dens.", "dp": 1, "units": "xx/kmâ€“2"},
                            "station_name":                        {"name_one": "Station Name"},
                            "country":                             {"name_one": "Country", "name_multiple": "Country"},
                            "area_classification":                 {"name_one": "Area Class.", "name_multiple": "Area Class."},
                            "station_classification":              {"name_one": "Station Class.", "name_multiple": "Station Class."},
                            "MODIS_MCD12C1_v6_IGBP_land_use":      {"name_one": "MODIS Land Use", "name_multiple": "MODIS Land Use"},
                            "GHSL_settlement_model_classification":{"name_one": "GHSL Class.", "name_multiple": "GHSL Class."}, 
                            "measurement_methodology":             {"name_one": "Method", "name_multiple": "Method"},
                            "measuring_instrument_name":           {"name_one": "Instrument", "name_multiple": "Instrument"}},
             "max_vars_per_row": 1,                 
             "max_uniques": 3,
             "var_spacing": 5,
             "remove_spines": ["top", "right", "left", "bottom"],
             "xtick_params": {"which": "both", "bottom": false, "top": false, "right": false, "left": false, "labelbottom": false, "labeltop": false, "labelright": false, "labelleft": false},
             "ytick_params": {"which": "both", "bottom": false, "top": false, "right": false, "left": false, "labelbottom": false, "labeltop": false, "labelright": false, "labelleft": false},
             "axis_title": {"label": "", "fontsize": 8.5, "loc": "left"},
             "xlabel": {"xlabel": ""},
             "ylabel": {"ylabel": ""},
             "plot": {"ha": "left", "va": "top", "fontsize": 8.0, "wrap": true, "linespacing": 1.5}},

"map": {"base_scale": 1.3,
        "map_coastline_resolution": "medium",
        "land_polygon": {"facecolor": "0.85"},
        "projection": "Robinson",
        "gridlines": {"linestyle": "-", "alpha": 0.4},
        "map_extent": [-180, 180, -90, 90],
        "axis_title": {"label": "", "fontsize": 8.5, "pad": 3, "loc": "center"},
        "cb": {"vmin_absolute": "", "vmax_absolute": "", "vmin_bias": "", "vmax_bias": "", "cmap_absolute": "", "cmap_bias": "", "n_ticks": 8, "orientation": "horizontal", "discrete": true, "n_discrete": 7},
        "cb_label": {"label": "", "fontsize": 8}, 
        "cb_tick_params": {"labelsize": 8},
        "annotate_stats": ["Mean"],
        "annotate_text": {"s": "", "loc": "upper right", "fontsize": 8, "n_stations": false, "round_decimal_places": 2},
        "annotate_bbox": {"facecolor": "white", "edgecolor": "gainsboro", "alpha": 1, "zorder": 200},
        "marker_selected": {"s": 8, "alpha": 1.0},
        "marker_unselected": {"s": 4, "alpha": 0.2},
        "plot": {"s": 4, "zorder": 3, "linewidth": 0.0, "alpha": null, "picker": 1},
        "plot_options": ["annotate"],
        "stations_annotate": {"xytext": [0, 0], "textcoords": "offset points", "size": 8, "ha": "center", "zorder": 100},
        "stations_annotate_bbox": {"boxstyle": "round", "linewidth": 0.5, "facecolor": "white", "edgecolor": "#757575"},
        "stations_annotate_arrowprops": {"frac": 1, "headwidth": 10, "shrink": 0.1, "color": "#757575", "edgecolor": "#5e5e5e"}},

"timeseries": {"grid": {"axis": "both", "color": "lightgrey", "alpha": 0.8},
               "remove_spines": ["top", "right"],
               "axis_title": {"label": "", "fontsize": 8.5, "loc": "left"}, 
               "xlabel": {"xlabel": "Time", "fontsize": 8},
               "ylabel": {"ylabel": "measurement_units", "fontsize": 8},
               "xtick_params": {"labelsize": 8, "which": "both", "bottom": true, "top": false, "right": false, "left": true, "labelbottom": true, "labeltop": false, "labelright": false, "labelleft": true},
               "ytick_params": {"labelsize": 8, "which": "both", "bottom": true, "top": false, "right": false, "left": true, "labelbottom": true, "labeltop": false, "labelright": false, "labelleft": true},
               "xtick_alteration":{"define": true, "n_slices": 6, "last_step": true},
               "annotate_stats": ["Mean", "StdDev", "r2", "RMSE", "MB"],
               "annotate_text": {"s": "", "loc": "upper right", "fontsize": 8, "n_stations": true, "round_decimal_places": 2},
               "annotate_bbox": {"facecolor": "white", "edgecolor": "gainsboro", "alpha": 1, "zorder": 100},
               "trend": {"window": 1, "min_points": 1, "format":{"linewidth": 1.0}},
               "plot": {"marker": "o", "markeredgecolor": null, "mew": 0, "linestyle": "None", "markersize": 2},
               "plot_options": ["annotate", "trend", "logy"]},

"periodic": {"axis_title": {"label": "", "fontsize": 8.5, "loc": "left"},
             "label": {"xy": [0, 1], "xytext": [2, -2], "xycoords": "axes fraction", "textcoords": "offset points", "fontsize": 9, "ha": "left", "va": "top", "zorder": 3},
             "ylabel": {"ylabel": "", "fontsize": 8}, 
             "xtick_params": {"labelsize": 8}, 
             "ytick_params": {"labelsize": 8, "pad": 0.0, "direction": "in"},
             "ygrid": {"color": "lightgrey", "alpha": 0.8, "zorder": 1},
             "annotate_stats": ["Min", "Max"],
             "annotate_text": {"s": "", "loc": "upper right", "fontsize": 8, "n_stations": false, "round_decimal_places": 2},
             "annotate_bbox": {"facecolor": "white", "edgecolor": "gainsboro", "alpha": 1, "zorder": 100},
             "bias_line": {"linestyle": "--", "linewidth": 1.0, "color": "black", "zorder": 2},
