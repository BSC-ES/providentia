                    "plot_options":["annotate"]},

"distribution": {"grid": {"axis": "both", "color": "lightgrey", "alpha": 0.8},
                 "remove_spines": ["top", "right"],
                 "axis_title": {"label": "", "fontsize": 8.5, "loc": "left"},
                 "xlabel": {"xlabel": "measurement_units", "fontsize": 8},
                 "ylabel": {"ylabel": "Density", "fontsize": 8}, 
                 "xtick_params": {"labelsize": 7, "which": "both", "bottom": true, "top": false, "right": false, "left": true, "labelbottom": true, "labeltop": false, "labelright": false, "labelleft": true},
